{% extends 'base.html.twig' %}

{% block title %}Nouvelle sortie{% endblock %}

{% block body %}
    <h1>Créer une nouvelle sortie</h1>

    {{ form_start(form) }}
    {{ form_widget(form) }}
    <button type="submit" class="btn btn-primary">Créer la sortie</button>
    <a href="{{ path('sortie_list') }}" class="btn btn-secondary">Retour à la liste</a>
    {{ form_end(form) }}
{% endblock %}
